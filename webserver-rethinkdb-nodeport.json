{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "name": "webserver-rethinkdb-nodeport"
    },
    "spec": {
        "type": "NodePort",
        "ports": [
            {
                "name": "28015-tcp",
                "port": 28015,
                "targetPort": 28015,
                "nodePort": 30018
            },
            {
                "name": "29015-tcp",
                "port": 29015,
                "targetPort": 29015,
                "nodePort": 30019
            }
        ],
        "selector": {
            "deploymentconfig": "rethinkdb"
        }
    }
}
